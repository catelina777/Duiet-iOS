# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'Duiet' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Duiet
  pod 'LicensePlist'
  pod 'R.swift'
  pod 'YPImagePicker'

  target 'DuietTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'R.swift'
  end

  target 'DuietUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'R.swift'
    pod 'YPImagePicker'
  end

end
